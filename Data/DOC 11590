  fontsize:M
  text:
  text:
  text:"1986: Test game season"
  text:"1987: AFL is founded"
  text:"* indicates Arena Bowl championship season"
  text:"Grey bars indicate non-AFL years or temporary hiatus"
  text:"2009: AFL suspends operations/restructures"

