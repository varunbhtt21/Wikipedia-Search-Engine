  id:Rep value:red                 legend:Republican
  id:Wh  value:yellow              legend:Whig
  id:NP  value:rgb(1,1,0.8)        legend:No_party
  id:Vac value:black               legend:Office_vacant

DateFormat = yyyy
Period     = from:1789 till:2021
