| rev8score = {{Rating|4|5}}<ref>{{cite journal |title=Banks: Goddess |work=[[Q (magazine)|Q]] |issue=339 |date=October 2014 |page=104}}</ref>
| rev9 = ''[[Rolling Stone]]''
