| score = 2 – 1
| team2 = [[Posta Rangers F.C.|Posta Rangers]] (2)
| report = [http://www.futaa.com//football/event/1861042 Report]
| goals1 = [[Enock Agwanda|Agwanda]] {{goal|51}} {{yel|69}}<br />[[Johnson Bagoole|Bagoole]] {{yel|67}}<br />[[Clifton Miheso|Miheso]] {{goal|90}}
| goals2 = [[Hashim Mukwana|Mukwana]] {{yel|44}}<br />[[Pascal Ochieng|Ochieng]] {{goal|57}}
| stadium = [[Afraha Stadium]], [[Nakuru]]
